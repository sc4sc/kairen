{
  "expo": {
    "name": "KAIREN",
    "slug": "kairen",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "notification": {
      "icon": "./assets/images/push-icon.png"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber" : "1.0.1",
      "supportsTablet": false,
      "bundleIdentifier": "kr.ac.kaist.kairen.kairen",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location information for emergency notification of accident."
      }
    },
    "android": {
      "package": "kr.ac.kaist.kairen.kairen",
      "versionCode": 2,
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    }
  }
}
